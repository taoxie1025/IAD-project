<div class="form-inline" align="center" >
    <%= image_tag("search_background.jpg", size: "600x180", :alt => "rss feed") %>
    <br><br><br>
    <%= form_tag("search", :method => "get", id: "search-form") do %>
        <%= text_field_tag :search, nil, :size => 110, placeholder: 
            'Enter course name or course number...', :class => 'form-control'%>
        <div style="width:80px">
            <%= submit_tag "Search", class: "btn btn-primary" %>
        </div>
    <% end %> 
</div>